@{
    ViewData["Title"] = "Zillow API";
}

<div>.</div>
<div>.</div>
<div>.</div>
<div>.</div>
<div class="col-md-4 col-md-offset-4">
    <form class="full-page-form" method="POST">
        @if (ViewBag.UserMessage != null)
            {
                <div class="text-muted">@ViewBag.UserMessage</div>

            }

        <div class="form-group">
            <label for="address-line1">Address Line 1</label>
            <input type="text" id="address-line1" asp-for="Address_Line1" placeholder="Address Line 1" />
        </div>
        <div class="form-group">
            <label for="address-line2">Address Line 2/label>
            <input type="text" id="address-line2" asp-for="Address_Line2" placeholder="Address Line 2" />
        </div>
        <div class="form-group">
            <label for="address-line3">Address Line 1</label>
            <input type="text" id="address-line3" asp-for="Address_Line3" placeholder="Address Line 3" />
        </div>
        <div class="form-group">
            <label for="price">Price</label>
            <input type="text" id="price" asp-for="Price" placeholder="Price" />
        </div>
        <input type="submit" value="Send" />
    </form>
</div>